.input {
  margin-top: 3rpx;
  width: 100%;
  border: 1px solid lightgray;
  border-radius: 6px;
}

.Normalpic {
  margin-top: 60rpx;
  width: 330rpx;
  height: 330rpx;
  /* border: none; 
  border:0 */
}
.Normalbtn
{ 
  margin-top: 60rpx;
